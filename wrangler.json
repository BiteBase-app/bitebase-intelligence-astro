{
  "name": "bitebase-intelligence-astro",
  "main": "./dist/index.js",
  "assets": {
    "directory": "./dist"
  },
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "vars": {
    "EXAMPLE_VALUE": "example_value"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "bitebase-intelligence-astro",
      "database_id": "5668b8aa-caa4-4656-b7a5-23d05e5acd8f"
    }
  ],
  "workflows": [
    {
      "name": "bitebase-intelligence-astro-customer-workflow",
      "binding": "CUSTOMER_WORKFLOW",
      "class_name": "CustomerWorkflow"
    }
  ]
}
